datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../prisma/generated/client"
}

model User {
  id        String   @id() @default(cuid())
  email     String   @unique()
  name      String?
  cellphone String[] @default([])
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt()
}
