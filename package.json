{
  "name": "loginapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run generate && next build",
    "only:build": " next build",
    "start": "NODE_PATH=./src && next start",
    "tsc": "tsc",
    "updatePackages": "ncu -u",
    "lint": "next lint",
    "generate": "npx zenstack generate",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config .prettierrc.js",
    "prisma:seed": "dotenv -e .env.local -- prisma db seed",
    "prisma:db-push": "dotenv -e .env.local -- prisma db push",
    "prisma:migrate-dev": "dotenv -e .env.local -- prisma migrate dev",
    "postinstall": "prisma generate --no-engine --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@better-fetch/fetch": "^1.1.18",
    "@clerk/localizations": "^3.16.5",
    "@clerk/nextjs": "^6.22.0",
    "@gfazioli/mantine-onboarding-tour": "^2.1.0",
    "@hookform/resolvers": "^5.1.1",
    "@mantine/core": "^8.1.0",
    "@mantine/dates": "^8.1.0",
    "@mantine/form": "^8.1.0",
    "@mantine/hooks": "^8.1.0",
    "@mantine/modals": "^8.1.0",
    "@mantine/notifications": "^8.1.0",
    "@mantine/nprogress": "^8.1.0",
    "@mantine/tiptap": "^8.1.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.8.2",
    "@prisma/extension-accelerate": "^2.0.1",
    "@tanstack/react-query": "^5.80.7",
    "@tiptap/extension-link": "^2.14.0",
    "@tiptap/pm": "^2.14.0",
    "@tiptap/react": "^2.14.0",
    "@tiptap/starter-kit": "^2.14.0",
    "@zenstackhq/runtime": "2.15.1",
    "@zenstackhq/server": "^2.15.1",
    "@zenstackhq/tanstack-query": "^2.15.1",
    "bcrypt": "^6.0.0",
    "better-auth": "^1.2.10",
    "dayjs": "^1.11.13",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "pg-pool": "^3.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "resend": "^4.6.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.3",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.0.1",
    "@types/pg-pool": "^2.0.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.5.5",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "6.8.x",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "zenstack": "2.15.1"
  },
  "description": "This is a Next.js application with ZenStack, Mantine, and Prisma integration, app used for login and management of users.",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
